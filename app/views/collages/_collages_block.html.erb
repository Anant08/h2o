<%= render :partial => 'shared/objects/collage', :collection => @collages.is_a?(WillPaginate::Collection) ? @collages : @collages[:results] %>

<% if request.xhr? -%>
<div id="new_pagination" style="display:none;">
<%= will_paginate @collages, :container => false, :previous_label => 'Prev', :next_label => 'Next' %>
</div>
<% end -%>
