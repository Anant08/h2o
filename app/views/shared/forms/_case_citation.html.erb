<div>
  <div id="error_block" style="display:none;" class="error"></div>
  <h3 id="generic_title"><%= @case_citation.new_record? ? 'Create a New Case Citation' : "Edit #{@case_citation}" %></h3>

  <% semantic_form_for(@case_citation, :html => { :id => 'case_citation-form'}) do |form| %>
    <% form.inputs do %>
      <%#= form.input :case_id %>
      <%= form.input :volume %>
      <%= form.input :reporter %>
      <%= form.input :page %>
    <% end -%>
  <% end -%>
</div>
