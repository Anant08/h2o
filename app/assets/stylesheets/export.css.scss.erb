#playlists_export, #collages_export, #text_blocks_export, 
#cases_export, #collages_export_unique, #journal_articles_export {
  .wrapper {
    max-width: 1050px;
    width: 1050px;
  }
  background: #FFF; font-family: sans-serif; font-size: 11px;  color: #000; border-bottom: 2px solid #FFF;
  * { 
    line-height: 1.4em;
    font-family: 'ff-dagny-web-pro';
    font-size: 14px;
  }
  .jsb-item {
    line-height: 2em;
  }
  #shell { background: #fff; }
  a { 
    color: #036290;
    text-decoration: none;
  }
  
  .singleitem {
    padding-top: 15px;
    float: none;
    width: 100%;
    div.article {
      padding-left: 0px;
      margin-top: 20px;
    }
    .details {
      background: none;
      padding: 0px;
      margin: 0px;
    }
    .scale0-8 {
      font-size: 11.2px;
    }
    .scale1-5 {
      font-size: 21px;
    }
    .scale1-3 {
      font-size: 18.2px;
    }
  }
  
  /* Header */
  .collage-content > h1 { margin-top: 0px; padding: 0px 0px 4px 0px; }
  .no_visible_children { display: none; }
  
  /* Timestamp & Author */
  .collage-content .info { padding: 0px; font-weight: bold; }
  
  /* Description */
  #playlist .details { padding-bottom: 10px; }
  #playlist .item_description,
  .collage-content .item_description { 
    font-style: italic; 
    color: #000;
    background: none;
    padding: 0px;
  }
  
  /* Collage Content */
  .unlayered-control,
  .collage-content { padding-left: 50px; clear: both; }
  .collage-content p { margin-bottom: 20px; }
  .collage-content .annotation-content { color: #6B933D; font-style: italic; border: 1px solid #d4d4d4; padding: 2px 5px 2px 18px; margin: 3px;
  background: url(<%= asset_path 'asterisk.jpg' %>) 2px 1px no-repeat; text-indent: 10px; width: 500px; display: none; }
  .collage-content .annotation-ellipsis { padding-left: 5px; }
  .collage-content p,
  .collage-content center,
  .collage-content h2 { position: relative; }
  .collage-content .paragraph-numbering {
    position: absolute; 
    left: -50px;
    text-decoration: none;
    font-weight: normal;
  }
  .collage-content blockquote { padding-left: 30px; margin-left: 30px; }
  .collage-content blockquote .paragraph-numbering { left: -50px; }
  
  .playlists .collage-content div.article { position: relative; }
  
  #playlist .playlists .playlists { display: block; }
  #playlist .listitem { padding-bottom: 10px; }
  #playlist .listitem .notes { padding-bottom: 10px; }
  .playlist h3 { margin-bottom: 10px; }
  .playlist p { margin-bottom: 10px; }
  
  /* Text Block */
  #text_block { clear: both; }
  #text_block .details { font-style: italic; margin-bottom: 20px; }
  #text_block div.article p { margin-bottom: 20px; }
  
  /* Print */
  #print-options {
    margin-top: 15px; 
    * {
      font-size: 12px;
    }
    .jsb-currentItem {
      line-height: 1.8em;
    }
  }
  #print-options .jsb {
    display: inline-block;
    float: none;
    border: 1px solid #666;
    border-radius:4px; 
    -moz-border-radius:4px; 
    -webkit-border-radius:4px;
    -o-border-radius:4px;
    margin: 0px 10px 5px 0px;
  }
  #fontface_span .jsb {
    width: 110px !important;
  }
  #print-options strong {
    display: inline-block;
    width: 150px;
    font-weight: normal;
  }
  #print-options #annotation_options,
  #print-options #highlight_options {
    display: inline-block;
  }
  #print-options #create_date,
  #print-options #paragraph_n {
    width: 200px;
  }
  #print-options #fontface_s {
    width: 176px;
  }
  #print-options #fontsize_s {
    width: 200px;
  }
  
  .print_border { display: block; position: absolute; width: 100%; height: 100%; top:0px; left: 0px; }
  .print_border span { width: 100%; display: block; position: absolute; bottom: 0px; opacity: 0.0; }
  
  .annotator-outer { display: none; }
  .layered-ellipsis { display: none; margin: 0px 2px; }

  .unlayered-ellipsis { display: none; margin: 0px 2px; }

  div.article {
    .annotator-wrapper {
      position: relative;
    }
    .layered-empty { 
      border-bottom: 2px solid #FEFED9;
    }
    .paragraph-numbering .layered-empty { 
      border-bottom: 2px solid #FFF;
    }
  }
  h3.missing_item {
    .number {
      margin-right: 20px;
    }
    .missing_item {
      opacity: 0.6;
      font-weight: normal;
    }
  }
}

.annotator-wrapper .annotation-indicator {
  display: none;
}
.annotator-wrapper > .unlayered-ellipsis {
  padding: 0px 0px 10px 502px;
}
