<h1>New annotation</h1>

<% form_for(@annotation) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :collage_id %><br />
    <%= f.text_field :collage_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :dom_element %><br />
    <%= f.text_field :dom_element %>
  </p>
  <p>
    <%= f.label :annotation %><br />
    <%= f.text_field :annotation %>
  </p>
  <p>
    <%= f.label :parent_id %><br />
    <%= f.text_field :parent_id %>
  </p>
  <p>
    <%= f.label :children_count %><br />
    <%= f.text_field :children_count %>
  </p>
  <p>
    <%= f.label :ancestors_count %><br />
    <%= f.text_field :ancestors_count %>
  </p>
  <p>
    <%= f.label :descendants_count %><br />
    <%= f.text_field :descendants_count %>
  </p>
  <p>
    <%= f.label :position %><br />
    <%= f.text_field :position %>
  </p>
  <p>
    <%= f.label :hidden %><br />
    <%= f.check_box :hidden %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', annotations_path %>